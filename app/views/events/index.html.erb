<h1>Dance Events</h1>

<%= link_to 'Create Event', new_event_path %>

<ul>
  <% @events.each do |event| %>
    <li>
      <%= link_to event.title, event_path(event) %>
      <p><%= event.description %></p>
      <p><%= event.start_time %></p>
      <p><%= event.location %></p>
    </li>
  <% end %>
</ul>
